package ua.sergey.kostyniuk;

public class Sort {  //преревірка на топ 6
	public  String s = " Номера з повторами | Номера з повторами | Номера з повторами | Номера з повторами | Номера з повторами | Номера з повторами | Номера з повторами |3606.|08367 3591 Номера з повторами |.08367 35|98.083| Номера з повторами |67 3601.08367";
	// public  static String[] str =;
	 public  static String[] m = new String[s.split("\\|").length];
	
	
    public static String pCh[] = new String[m.length]; //Массив предыдущих чисел

    
    
    public static int ts;
    public static int tss;
    public static int[] n = new int[m.length];   //Номера з повторами
   
    
    
    
    public static void main(String[] args) {  
  
        int k = 0;

		String ch;
        for(ts = 0; ts<m.length; ts++){
        	
            if (!povtor(ch=m[ts])){              //чи не повторювалось вже
                ch=m[ts];
                for(int ts2=0;ts2<m.length;ts2++)             
                    if(m[ts2]==ch){   
                        n[k]=ts2+1;  
                        k++;         //кількість повторів
                    }
                if (k>2){            //якщо більше 6
                    System.out.print("то 6 найбільш вживаних фраз ");
                  //  for(int a=0;a<k;a++) System.out.print(" "+n[a]);
                    System.out.println(": "+ch);
                }
                k=0; //Обнуляємо
                pCh[ts]=ch; 
            }
            
        }
    }
    static boolean povtor(String i){          
        for(int a = 0; a<ts;a++){
            if (pCh[a]==i) return true;
        }
        return false;
    }
 
}


