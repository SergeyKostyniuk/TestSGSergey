package ua.sergey.kostyniuk;

public class Test {
     public static void main(String[] args){
    	 System.out.println(fib(3));
     }

	private static long fib(int i) {
		 int[] fibArray = new int[i + 1];
	        fibArray[0] = 0;
	        fibArray[1] = 1;
	        for (int i1 = 2; i1 < fibArray.length; i1++) {
	            fibArray[i1] = fibArray[i1 - 1] + fibArray[i1 - 2];
	        }
	        return fibArray[fibArray.length - 1];
	    
	}
}
